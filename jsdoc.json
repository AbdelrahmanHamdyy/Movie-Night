{
  "source": {
    "include": [
      "./src/middlewares",
      "./src/models",
      "./src/utils",
      "./src/services"
    ],
    "includePattern": ".ts$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["plugins/markdown", "jsdoc-plugin-typescript"],
  "opts": {
    "recurse": true,
    "destination": "./docs/",
    "readme": "./README.md"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "typescript": {
    "moduleRoot": "src"
  }
}
